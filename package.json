{
  "name": "rpdad",
  "version": "1.0.0",
  "description": "Website template for Payload",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types && next build",
    "postbuild": "next-sitemap --config next-sitemap.config.cjs",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "dev:prod": "cross-env NODE_OPTIONS=--no-deprecation rm -rf .next && pnpm build && pnpm start",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "ii": "cross-env NODE_OPTIONS=--no-deprecation pnpm --ignore-workspace install",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "lint:fix": "cross-env NODE_OPTIONS=--no-deprecation next lint --fix",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "reinstall": "cross-env NODE_OPTIONS=--no-deprecation rm -rf node_modules && rm pnpm-lock.yaml && pnpm --ignore-workspace install",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@payloadcms/admin-bar": "3.42.0",
    "@payloadcms/db-postgres": "3.42.0",
    "@payloadcms/email-nodemailer": "3.42.0",
    "@payloadcms/email-resend": "3.42.0",
    "@payloadcms/live-preview-react": "3.42.0",
    "@payloadcms/next": "3.42.0",
    "@payloadcms/payload-cloud": "3.42.0",
    "@payloadcms/plugin-form-builder": "3.42.0",
    "@payloadcms/plugin-nested-docs": "3.42.0",
    "@payloadcms/plugin-redirects": "3.42.0",
    "@payloadcms/plugin-search": "3.42.0",
    "@payloadcms/plugin-seo": "3.42.0",
    "@payloadcms/richtext-lexical": "3.42.0",
    "@payloadcms/storage-s3": "3.42.0",
    "@payloadcms/translations": "3.42.0",
    "@payloadcms/ui": "3.42.0",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slot": "^1.2.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "d3": "^7.9.0",
    "d3-geo": "^3.1.1",
    "framer-motion": "^12.19.1",
    "geist": "^1.4.2",
    "graphql": "^16.11.0",
    "lucide-react": "^0.378.0",
    "next": "15.3.0",
    "next-sitemap": "^4.2.3",
    "nodemailer": "^7.0.3",
    "payload": "3.42.0",
    "prism-react-renderer": "^2.4.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "7.57.0-next.0",
    "react-player": "^3.3.3",
    "sharp": "0.32.6",
    "tailwind-merge": "^2.6.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@tailwindcss/postcss": "^4.1.10",
    "@tailwindcss/typography": "^0.5.16",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/d3": "^7.4.3",
    "@types/escape-html": "^1.0.4",
    "@types/node": "22.5.4",
    "@types/react": "19.1.0",
    "@types/react-dom": "19.1.2",
    "autoprefixer": "^10.4.21",
    "copyfiles": "^2.4.1",
    "eslint": "^9.29.0",
    "eslint-config-next": "15.3.0",
    "eslint-config-prettier": "^10.1.5",
    "postcss": "^8.5.6",
    "prettier": "^3.6.0",
    "prettier-plugin-tailwindcss": "^0.6.13",
    "tailwindcss": "^4.1.10",
    "tw-animate-css": "^1.3.4",
    "typescript": "5.7.3"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0",
    "pnpm": "^9 || ^10"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@tailwindcss/oxide",
      "sharp"
    ]
  }
}
